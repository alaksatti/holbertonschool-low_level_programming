#include <stdio.h>





int main(void)
{
	int i, j, p;

	for (i = 0; i <= 9; ++i)
		for (j = 0; j <= 9; ++j)
		{
			p = i * j;

			if (j == 0)
			{
				putchar('0');
			}

			else if (p > 9)
			{
				putchar(' ');
				putchar(p / 10 + '0');
				putchar(p % 10 + '0');
			}
			else
			{
				putchar(' ');
				putchar(' ');
				putchar(p + '0');
			}
			if (!(j == 9))
			    putchar(',');
			else
				putchar('\n');
		}
			return (0);
}
