/**
 add - adds two number together.
 @a: first number.
 @b: second number.
 Return: addition.
 */


int add(a, b)
{
	return (a + b);
}


/**
 * sub - subtracts two numbers.
 * @a: first number.
 * @b: second number.
 * Return: Result.
 */
int sub(a, b)
{

	return (a - b);

}

/**
 * mul - multiplies two numbers.
 * @a: first number.
 * @b: second number.
 * Return: Result.
 */
int mul(a, b)
{
	return (a * b);
}
/**
 * div - divides two numbers.
 * @a: first number.
 * @b: second number.
 * Return: Result.
 */

int div(a, b)
{
	return (a / b);
}
