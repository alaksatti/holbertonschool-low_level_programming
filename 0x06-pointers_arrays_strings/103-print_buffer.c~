#include <stdio.h>
void print_buffer(char *b, int size)
{

	int i, j;

	if (size <= 0)
		putchar('\n');

	for (i = 0; i <= size; i++)
		{
			if (((b[j] >= 'A' || b[j] >= 'a') && (b[j] <= 'z'
|| b[j] <= 'Z')) || b[j] == '!' || b[j] == '#' || b[j] == '$' || b[j] == '%'
|| b[j] == '*' || b[j] == ')' || b[j] == ':' || b[j] == ';' || b[j] == '&'
|| b[j] == '(' || b[j] == '&')
				printf("%c\n", b[i]);
			else
				printf(".\n");
		}

}
